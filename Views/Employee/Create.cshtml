@model EmployeeEFCoreCodeFirstApp.Models.Employee
<h2>Create</h2>
<form asp-action="Create" method="Post">
    <div class="form-group">
        <label asp-for="EmployeeName" class="form-control col-sm-2">Enter Name</label>
        <input asp-for="EmployeeName" class="form-control col-sm-10" />
    </div>

    <div class="form-group">
        <label asp-for="DepartmentName" class="form-control col-sm-2">Enter Department</label>
        <input asp-for="DepartmentName" class="form-control col-sm-10" />
    </div>

    <div class="form-group">
        <label asp-for="Age" class="form-control col-sm-2">Enter Age</label>
        <input asp-for="Age" class="form-control col-sm-10" />
    </div>

    <div class="form-group">
        <label asp-for="Address" class="form-control col-sm-2">Enter Address</label>
        <input asp-for="Address" class="form-control col-sm-10" />
    </div>

    <div class="form-group">
        <label asp-for="Contact" class="form-control col-sm-2">Enter Contact</label>
        <input asp-for="Contact" class="form-control col-sm-10" />
    </div>
    <div>
        <input type="submit" class="btn btn-success" value="Create" />
    </div>

</form>